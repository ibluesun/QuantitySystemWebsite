    jQuery(function($, undefined) {
    $('#term_demo').terminal(function(command, term) {
            if (command !== '') {
                try {

                    if (command == 'copyright') {
                        term.echo(copyright);
                    }
                    else {
                        $.post("/api/Qs/", { '': command })
                        .done(function (data) {
                            term.echo(data);
                            term.echo('');
                        })
                        .fail(function (d, textStatus, error) {
                            //alert(error);
                            var err = eval('(' + d.responseText + ')');
                            term.error(new String(err.Message));
                        })
                    }
                    ;
                } catch(e) {
                    term.error(new String(e));
                }
            } else {
                term.echo('');
            }
            }, {
                greetings: copyright,
                name: 'Qs_Demo',
                height: 480,
                prompt: 'Qs> '
            });


    });


    

</script>

<div class="row">
    <div class="col-md-12">
