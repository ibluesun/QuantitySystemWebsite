@{
    ViewBag.Title = "Units";

    var quantity_units = QsWeb.AvailableUnits();


}

<div class="jumbotron">
    <h3>Available Units [@quantity_units.Count()]</h3>
</div>


<div class="row">
    <div class="col-md-12">

        <table class="table table-striped">
            <thead>
                <tr>
                    <td>#</td>
                    <td>Name</td>
                    <td>Symbol</td>
                    <td>System</td>
                    <td>Quantity Type</td>
                </tr>
            </thead>

            @for (int ix = 0; ix < quantity_units.Length; ix++)
            {
                var v = quantity_units[ix];
                <tr>
                    <td>@ix</td>
                    <td>@v.Name</td>
                    <td>@v.Symbol</td>
                    <td>@v.System</td>
                    <td>@v.QuantityType</td>
                </tr>

            }
        </table>
    </div>
</div>